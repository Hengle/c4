#!/usr/bin/env python
import sys

from c4.engine import NegamaxEngine
from c4.game import GameHandler


def main():
    engine1 = NegamaxEngine(3)
    engine2 = NegamaxEngine(2)

    game = GameHandler(engine1, engine2, verbose=True)
    b = game.play()
    print b
        
    
if __name__ == '__main__':
    sys.exit(main())
