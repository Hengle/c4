#!/usr/bin/env python
import sys

from c4.board import DRAW
from c4.engine import NegamaxEngine, AlphaBetaEngine
from c4.game import GameHandler
from c4.arena import arena


def main():
    engines = [AlphaBetaEngine(2),
               NegamaxEngine(2)]
    arena(engines)
        
    
if __name__ == '__main__':
    sys.exit(main())
